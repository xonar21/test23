{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "vue@2.7.14",
      "destination": "wwwroot/lib/vue/"
    },
    {
      "provider": "unpkg",
      "library": "vuex@3.6.2",
      "destination": "wwwroot/lib/vuex/"
    },
    {
      "library": "vuex-persistedstate@4.1.0",
      "destination": "wwwroot/lib/vuex-persistedstate/"
    },
    {
      "provider": "unpkg",
      "library": "axios@1.3.4",
      "destination": "wwwroot/lib/axios/"
    },
    {
      "library": "vue-clickaway@2.2.1",
      "destination": "wwwroot/lib/vue-clickaway/"
    },
    {
      "library": "bootstrap-select@1.13.18",
      "destination": "wwwroot/lib/bootstrap-select/"
    },
    {
      "library": "jquery-contextmenu@2.9.2",
      "destination": "wwwroot/lib/jquery-contextmenu/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "Vue.Draggable@2.24.3",
      "destination": "wwwroot/lib/Vue.Draggable/"
    },
    {
      "library": "Sortable@1.15.0",
      "destination": "wwwroot/lib/Sortable/"
    },
    {
      "library": "jspdf@2.5.1",
      "destination": "wwwroot/lib/jspdf/"
    },
    {
      "library": "html2canvas@1.4.1",
      "destination": "wwwroot/lib/html2canvas/"
    },
    {
      "library": "html2pdf.js@0.10.1",
      "destination": "wwwroot/lib/html2pdf.js/"
    },
    {
      "library": "es6-promise@4.2.8",
      "destination": "wwwroot/lib/es6-promise/"
    },
    {
      "library": "feather-icons@4.29.0",
      "destination": "wwwroot/lib/feather-icons/"
    },
    {
      "library": "vue-chartjs@5.2.0",
      "destination": "wwwroot/lib/vue-chartjs/"
    },
    {
      "provider": "unpkg",
      "library": "chart.js@2.9.3",
      "destination": "wwwroot/lib/Chart.js"
    },
    //{
    //  "provider": "jsdelivr",
    //  "library": "vue-handy-scroll@1.0.1",
    //  "destination": "wwwroot/lib/vue-handy-scroll/"
    //},
    {
      "library": "microsoft-signalr@7.0.3",
      "destination": "wwwroot/lib/microsoft-signalr/"
    }

  ]
}
