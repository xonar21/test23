{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "grapesjs@0.20.4",
      "destination": "wwwroot/lib/grapesjs/"
    },
    {
      "provider": "unpkg",
      "library": "grapesjs-preset-webpage@0.1.11",
      "destination": "wwwroot/lib/grapesjs-preset-webpage/"
    },
    {
      "library": "jqtree@1.7.0",
      "destination": "wwwroot/lib/jqtree/",
      "files": [
        "tree.jquery.js",
        "jqtree.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-switch@3.3.5",
      "destination": "wwwroot/lib/bootstrap-switch/"
    },
    {
      "library": "moment.js@2.29.4",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    }
  ]
}
